        "theta_std = np.zeros((X_train_std.shape[1], 1))\n",
        "iterations = 1500\n",
        "alpha = 0.1\n",
        "theta_std, train_cost_std, test_cost_std = gradient_descent(X_train_std, y_train, X_test_std, y_test, theta_std, alpha, iterations)\n",
